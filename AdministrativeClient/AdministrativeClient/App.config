<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IWebService">
                    <security mode="None">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:4321/AdministrativeService"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWebService"
                contract="ServiceReference.IWebService" name="BasicHttpBinding_IWebService" />
        </client>
    </system.serviceModel>
</configuration>
